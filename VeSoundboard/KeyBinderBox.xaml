<UserControl x:Name="baseKeyBinderBox" x:Class="VeSoundboard.KeyBinderBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="100" Height="23" Width="120" HorizontalAlignment="Left" VerticalAlignment="Top" Loaded="baseKeyBinderBox_Loaded">
    <TextBox x:Name="baseTextBox" IsReadOnly="True" Text="&lt;No Key&gt;" TextAlignment="Center" MouseDoubleClick="baseTextBox_MouseDoubleClick" LostFocus="baseTextBox_LostFocus" PreviewKeyDown="UserControl_KeyDown"/>
</UserControl>
